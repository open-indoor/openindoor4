:80 {
    root * /openindoor
    file_server

    log {
    	level DEBUG
    }

    route /overpass/* {
    	uri strip_prefix /overpass
	    reverse_proxy {
            to http://www.overpass-api.de:80
        }
    }

    route /fonts/* {
    	uri strip_prefix /fonts
	    reverse_proxy {
            to http://fonts:80
        }
    }

}
