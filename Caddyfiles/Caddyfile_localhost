:80 {
    encode gzip
    root * /openindoor
    file_server

    log {
    	level DEBUG
    }

    route /data/* {
	    reverse_proxy {
            to http://tileserver:80
        }
    }

    route /overpass/* {
    	uri strip_prefix /overpass
	    reverse_proxy {
            to http://overpassCache:80
        }
    }

    route /rastertiles/* {
    	uri strip_prefix /rastertiles
	    reverse_proxy {
            to http://tileCache:80
        }
    }

    route /fonts/* {
    	uri strip_prefix /fonts
	    reverse_proxy {
            to http://fonts:80
        }
    }

}