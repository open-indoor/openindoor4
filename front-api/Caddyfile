${DOMAIN_NAMES} {

    root * /data/www
    file_server browse

    tls contact@openindoor.io {
        ca ${CERTIFICATE_AUTHORITY}
    }

    log {
        level DEBUG
    }

    route /osm/* {
        reverse_proxy {
            to http://osm-api:80
        }
    }

    route /places/* {
        reverse_proxy {
            to http://places-api:80
        }
    }

    route /bboxes* {
        reverse_proxy {
            to http://osm-api:80
        }
    }

    route /geojson* {
	    reverse_proxy {
            to http://geojson-api:80
        }
    }

    route /mbtiles/* {
	    reverse_proxy {
            to http://mbtiles-api:80
        }
    }

    route /mbtiles/country/* {
	    reverse_proxy {
            to http://mbtiles-country-api:80
        }
    }

    route /tileserver/* {
	    reverse_proxy {
            to http://tileserver-api:80
        }
    }

}