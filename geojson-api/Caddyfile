:80 {

    log

    handle_errors {
        @404 {
            expression {http.error.status_code} == 404
        }

        respond @404 "File not found."
    }

    reverse_proxy /geojson* :8999 {
        transport fastcgi {
            split geojson
            env API geojson-api
            env DOMAIN_NAME ${DOMAIN_NAME}
        }
    }
}