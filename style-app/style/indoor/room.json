{
    "id": "${roomId}",
    "source-layer": "osm-indoor",
    "source": "building",
    "type": "fill-extrusion",
    "minzoom": $INDOOR_MIN_ZOOM,
    "filter": $roomFilter,
    "paint": {
        "fill-extrusion-color": "${roomColor}",
        "fill-extrusion-height": $roomHeight,
        "fill-extrusion-base": 0,
        "fill-extrusion-opacity": 0.5
    }
}