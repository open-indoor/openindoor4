{
  "version": 8,
  "id": "openindoor",
  "name": "openindoor",
  "center": [
    0,
    0
  ],
  "sprite": "https://${APP_DOMAIN_NAME}/sprite/sprite",
  "glyphs": "fonts/{fontstack}/{range}.pbf",
  "sources": {
    "none": {
      "type": "vector",
      "url": ""
    },
    "raster-tiles": {
      "type": "raster",
      "tiles": [
        "https://${APP_DOMAIN_NAME}/rastertiles/{z}/{x}/{y}.png"
      ],
      "tileSize": 256,
      "maxzoom": 19
    },
    "building":
    {
      "type": "vector",
      "url": "https://${API_DOMAIN_NAME}/tileserver/data/${country}.json"

    },
    "indoor":
    {
      "type": "vector",
      "url": "https://${API_DOMAIN_NAME}/tileserver/data/${country}.json"

    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#1d1f20"
      }
    },
    {
      "id": "simple-tiles",
      "type": "raster",
      "source": "raster-tiles",
      "minzoom": 0,
      "maxzoom": 19
    },
    "buildingLayers",
    "indoorLayers"
  ]
}