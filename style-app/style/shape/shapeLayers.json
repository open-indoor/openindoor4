[
  {
    "id": "building-part-extrusion",
    "source-layer": "osm-indoor",
    "source": "building",
    "type": "fill-extrusion",
    "minzoom": $SHAPE_MIN_ZOOM,
    "filter": [
      "any",
      [
        "has",
        "building:part"
      ]
    ],
    "paint": {
      "fill-extrusion-color": "#FFFFFF",
      "fill-extrusion-height": [
        "interpolate",
        [
          "linear"
        ],
        [
          "zoom"
        ],
        17,
        [
					"case",
					[
						"has",
						"height"
					],
					[
						"to-number",
						[
							"get",
							"height"
						]
					],
					30
				],
        18,
        0
      ],
      "fill-extrusion-base": [
        "interpolate",
        [
          "linear"
        ],
        [
          "zoom"
        ],
        17,
        [
					"case",
					[
						"has",
						"min_height"
					],
					[
						"to-number",
						[
							"get",
							"min_height"
						]
					],
					0
				],
        18,
        0
      ],
      "fill-extrusion-opacity": 0.5
    }
  }
]