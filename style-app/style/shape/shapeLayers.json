[
  {
    "id": "shape-area-extrusion-area",
    "source-layer": "osm-indoor",
    "source": "shape",
    "type": "fill-extrusion",
    "minzoom": $SHAPE_MIN_ZOOM,
    "maxzoom": $SHAPE_MAX_ZOOM,
    "filter": [
      "all",
      [
        "==",
        [
          "get",
          "indoor"
        ],
        "area"
      ],
      [
        "has",
        "level"
      ],
      [
        "==",
        [
          "index-of",
          ";",
          [
            "get",
            "level"
          ]
        ],
        -1
      ],
      [
        ">=",
        [
          "to-number",
          [
            "get",
            "level"
          ]
        ],
        0
      ]
    ],
    "paint": {
      "fill-extrusion-color": "#FF0000",
      "fill-extrusion-height": [
        "*",
        3,
        [
          "to-number",
          [
            "get",
            "level"
          ]
        ]
      ],
      "fill-extrusion-base": [
        "*",
        3,
        [
          "to-number",
          [
            "get",
            "level"
          ]
        ]
      ],
      "fill-extrusion-opacity": 0.5
    }
  },
  {
    "id": "shape-area-extrusion-room",
    "source-layer": "osm-indoor",
    "source": "shape",
    "type": "fill-extrusion",
    "minzoom": $SHAPE_MIN_ZOOM,
    "maxzoom": $SHAPE_MAX_ZOOM,
    "filter": [
      "all",
      [
        "==",
        [
          "get",
          "indoor"
        ],
        "room"
      ],
      [
        "has",
        "level"
      ],
      [
        "==",
        [
          "index-of",
          ";",
          [
            "get",
            "level"
          ]
        ],
        -1
      ],
      [
        ">=",
        [
          "to-number",
          [
            "get",
            "level"
          ]
        ],
        0
      ]
    ],
    "paint": {
      "fill-extrusion-color": "#0000FF",
      "fill-extrusion-height": [
        "+",
        1,
        [
          "*",
          3,
          [
            "to-number",
            [
              "get",
              "level"
            ]
          ]
        ]
      ],
      "fill-extrusion-base": [
        "+",
        1,
        [
          "*",
          3,
          [
            "to-number",
            [
              "get",
              "level"
            ]
          ]
        ]
      ],
      "fill-extrusion-opacity": 0.5
    }
  }
]